# UAVScope Environment Variables
# Copy this file to .env and update with your actual values

# TATOR API Configuration
# The base URL for the TATOR API server
VITE_TATOR_HOST=http://localhost

# TATOR API Token
# Your authentication token for the TATOR API
VITE_TATOR_TOKEN=your_tator_api_token_here

# TATOR Project Configuration
# The project ID to fetch data from
VITE_PROJECT_ID=4

# The box type ID for detections/localizations
VITE_BOX_TYPE=3

# API Configuration
# Request timeout in milliseconds
VITE_API_TIMEOUT=3000

# Maximum number of retry attempts for failed requests
VITE_MAX_RETRIES=3
